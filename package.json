{
  "name": "datamint",
  "version": "1.0.0",
  "description": "Dockerized databases for development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "git+https://github.com/emilohlund-git/datamint.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy",
    "prepare": "npm run build",
    "copy": "copyfiles -u 1 src/docker/* dist",
    "test": "jest",
    "test:logging": "jest --detectOpenHandles",
    "test:debug": "node runJest.js --inspect --runInBand",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "docker",
    "database",
    "development"
  ],
  "author": "Emil Ã–lund",
  "license": "ISC",
  "devDependencies": {
    "@types/heapdump": "^0.3.4",
    "@types/jest": "^29.5.12",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^20.11.27",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "semantic-release": "^23.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "heapdump": "^0.3.15",
    "mongodb": "^6.5.0",
    "mysql2": "^3.9.2",
    "pg-promise": "^11.5.4",
    "react-icons": "^5.0.1",
    "rimraf": "^5.0.5"
  }
}
